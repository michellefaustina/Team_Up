<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sourcey.TeamUp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".TeamUp">

        <activity
            android:name=".DispatchActivity"
            android:theme="@style/AppTheme.Dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.Dark"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".SignupActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name=".AvailableTimeActivity" android:theme="@style/AppTheme.Light"/>
        <activity android:name=".MyTimeActivity" android:theme="@style/AppTheme.Light"/>
        <activity android:name=".ProfileActivity" android:theme="@style/AppTheme.Light"/>
        <activity android:name=".PopupActivity"/>
        <activity android:name=".DetailsATActivity"/>
        <activity android:name=".DetailsMTActivity"/>
        <activity android:name=".LoginActivity"  android:theme="@style/AppTheme.Light"/>
        <activity android:name=".EmailVerifyActivity" android:theme="@style/AppTheme.Light"/>
        <activity android:name=".ElementtimesActivity"/>
        <activity android:name=".ElementmytimesActivity"/>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>
